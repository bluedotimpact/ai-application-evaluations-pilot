  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/bluedotimpact/ai-application-evaluations-pilot.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "shx cp -n .env.template .env && shx cp -n demo.csv data.csv",
