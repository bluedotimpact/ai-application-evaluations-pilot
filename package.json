    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-domdomegg"
    ]
  },
  "dependencies": {
    "axios": "^1.4.0",
    "better-sqlite3": "^8.5.0",
    "csv-parse": "^5.4.0",
    "csv-stringify": "^6.4.0",
    "dotenv": "^16.3.1",
    "kysely": "^0.26.1",
    "p-limit": "^3.1.0",
    "p-retry": "^4.6.2",
